{
  "name": "sophisticate",
  "version": "0.1.1",
  "author": "Frédéric Maquin <fred@ephread.com> (http://ephread.com)",
  "description": "Sophisticate the output from SVG editors, to display them within interactive web pages.",
  "keywords": [
    "SVG",
    "svg",
    "transform",
    "preprocess",
    "process",
    "convert",
    "prepare"
  ],
  "bin": {
    "sophisticate": "./bin/sophisticate"
  },
  "scripts": {
    "build": "tsc -p ./",
    "watch": "tsc -w -p ./",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "prepublishOnly": "npm run-script build"
  },
  "bugs": "https://github.com/ephread/sophisticate/issues",
  "repository": "ephread/sophisticate",
  "devDependencies": {
    "@types/yargs": "^13.0.0",
    "@types/js-yaml": "^3.12.1",
    "@types/svgo": "^1.0.1",
    "@types/ejs": "^2.6.3",
    "@types/libxmljs": "^0.18.3"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "ejs": "^2.6.1",
    "js-yaml": "^3.13.1",
    "libxmljs": "^0.19.5",
    "svgo": "^1.2.1",
    "yargs": "^13.2.2"
  },
  "license": "MIT"
}
