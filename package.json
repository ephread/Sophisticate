{
  "name": "sophisticate",
  "version": "0.1.0",
  "author": "Frédéric Maquin <fred@ephread.com> (http://ephread.com)",
  "description": "Sophisticate the output from SVG editors, to display them within interactive web pages.",
  "keywords": [
    "SVG", "svg", "transform",
    "preprocess", "process",
    "convert", "prepare"
  ],
  "bin": {
    "sophisticate": "./bin/sophisticate"
  },
  "scripts": {
    "compile": "babel --presets es2015 -d lib/ src/",
    "prepublish": "npm run compile"
  },
  "bugs": "https://github.com/ephread/sophisticate/issues",
  "repository": "ephread/sophisticate",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "eslint": "^3.18.0",
    "eslint-config-standard": "^7.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "chalk": "^1.1.3",
    "ejs": "^2.5.6",
    "js-yaml": "^3.8.1",
    "libxmljs": "^0.18.4",
    "svgo": "^0.7.2",
    "yargs": "^6.6.0"
  },
  "license": "MIT"
}
