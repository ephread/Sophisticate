<!doctype html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0,
                                 user-scalable=no, minimum-scale=1.0,
                                 maximum-scale=1.0">
</head>
<body>
  <% svg.forEach(function(svgElement.name, index) { %>
    <div class="<%= (index % 2 == 0 ? 'even' : 'odd') %>">
      <%- svgElement.content %>
    </div>
  <% }); %>
</body>
</html>
